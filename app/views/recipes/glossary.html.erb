<% provide(:title, "Recipe Glossary") %>

<div class="container">
  <div class="row">
    <div class="col-12">
      <h2>Recipe Glossary Page</h2>
      <h4>Check recipe names in our glossary here!</h4>
    </div>
  </div>
  
  <div class = "row">
    <div class="col-xm-12">
      <%= alphabetical_paginate @alphaParams %>
      
      <br>
        
      <div id="pagination_table" class="col-xm-12">
        <br>
        <div id="results" class="col-xm-12">
          <%= render 'results' %>
        </div>
        <br>
      </div>
      
    </div>
  </div>
</div>
